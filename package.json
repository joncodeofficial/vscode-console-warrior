{
  "name": "console-warrior",
  "displayName": "console warrior",
  "description": "it is an free alternative to console ninja open source",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "console-warrior.testConsole",
        "title": "Console Warrior: Run Console in Port"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/portscanner": "^2.1.4",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.19.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@types/http-proxy": "^1.17.16",
    "@types/open": "^6.2.1",
    "@types/ws": "^8.5.14",
    "chalk": "^5.4.1",
    "http-proxy": "^1.18.1",
    "kleur": "^4.1.5",
    "node-fetch": "^3.3.2",
    "open": "^10.1.0",
    "playwright": "^1.50.1",
    "portscanner": "^2.2.0",
    "ws": "^8.18.0"
  }
}
