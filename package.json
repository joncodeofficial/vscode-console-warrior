{
  "name": "console-warrior",
  "displayName": "console warrior",
  "description": "it is an free similar to console ninja open source",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Programming Languages",
    "Linters",
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "console-warrior.runPort",
        "title": "Enter a port number to launch Console Warrior"
      },
      {
        "command": "extension.runScript",
        "title": "Testing command to run a script"
      },
      {
        "command": "extension.injectConsoleWarriorPlugin",
        "title": "Inyectar Console Warrior Plugin en Vite"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/http-proxy": "^1.17.16",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/portscanner": "^2.1.4",
    "@types/vscode": "^1.97.0",
    "@types/ws": "^8.5.14",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.19.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "http-proxy": "^1.18.1",
    "portscanner": "^2.2.0",
    "source-map": "^0.7.4",
    "ws": "^8.18.0"
  },
  "packageManager": "pnpm@10.8.1+sha512.c50088ba998c67b8ca8c99df8a5e02fd2ae2e2b29aaf238feaa9e124248d3f48f9fb6db2424949ff901cffbb5e0f0cc1ad6aedb602cd29450751d11c35023677"
}
