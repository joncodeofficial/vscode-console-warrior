{
  "name": "console-warrior-logs",
  "displayName": "Console Warrior Logs",
  "description": "Visualize console.log messages directly in VSCode editor inline, boosting debugging and productivity.",
  "version": "0.0.20",
  "publisher": "Jonpena",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Visualization",
    "Linters",
    "Debuggers"
  ],
  "activationEvents": [
    "workspaceContains:package.json"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.addPort",
        "title": "Connect to Console Warrior Port"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile && sh ./src/scripts/add_version.sh 0.0.20",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/http-proxy": "^1.17.16",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/portscanner": "^2.1.4",
    "@types/vscode": "^1.97.0",
    "@types/ws": "^8.5.14",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.19.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@jridgewell/trace-mapping": "^0.3.25",
    "denque": "^2.1.0",
    "http-proxy": "^1.18.1",
    "portscanner": "^2.2.0",
    "ws": "^8.18.0"
  },
  "packageManager": "pnpm@10.8.1"
}
